
@{
    ViewData["Title"] = "ASP- Admin Panel";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

@*<h1>This is Admin Template intgerated inside ASP.NET CORE MVC</h1>*@

<link href="~/css/Style.css" rel="stylesheet" />

<div class="row mt-2">
    <div class="col-xl-3 col-md-6">
        <div class="card bg-primary text-black mb-4">
            <div class="card-body">
                <h3 class="text-center font-weight-bold hStyle">Total Blogs</h3>
                <p class="text-center font-weight-bold h4 ">500</p>
            </div>
            @*<div class="card-footer d-flex align-items-center justify-content-between">
                    <a class="small text-white stretched-link" href="#">View Details</a>
                    <div class="small text-white"><i class="fas fa-angle-right"></i></div>
                </div>*@
        </div>
    </div>
    <div class="col-xl-3 col-md-6">
        <div class="card bg-warning text-black mb-4">
            <div class="card-body">
                <h3 class="text-center font-weight-bold hStyle">Total Views</h3>
                <p class="text-center font-weight-bold h4 ">6,374</p>
            </div>
